{
  "name": "Fluctus",
  "productName": "Fluctus",
  "version": "2017.8.100",
  "description": "Watch videos from youtube(and more) in a floating window",
  "author": "Vik Borges <v1k@protonmail.com>",
  "copyright": "Â© 2017, Vik Borges.",
  "homepage": "https://kivs.github.io/Fluctus",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=dev electron .",
    "rebuild": "electron-rebuild -v 1.8.1",
    "pack": "build --dir --win --linux --x64",
    "dist": "build --win --linux --x64 --ia32",
    "publish": "build --win --linux --x64 --ia32 -p always",
    "postinstall": "install-app-deps",
    "test": "NODE_ENV=test mocha tests/test.js"
  },
  "build": {
    "appId": "vik.my.fluctus",
    "win": {},
    "publish": [
      {
        "provider": "s3",
        "owner": "v1k",
        "bucket": "floating-dog",
        "path": "native_app_updates"
      }
    ]
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "electron-updater": "^2.4.3",
    "node-fetch": "^1.7.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "^13.6.0",
    "electron-publisher-s3": "^15.0.1",
    "electron-rebuild": "^1.5.7",
    "mocha": "^4.0.1",
    "spectron": "^3.6.0",
    "typescript": "^2.4.1"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
